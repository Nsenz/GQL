{
  "name": "gqlclient",
  "version": "1.0.0",
  "description": "GQL NTCS",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "webpack --mode production --no-devtool",
    "start": "webpack serve --open",
    "getschema": "npx apollo service:download --endpoint=http://localhost:4001/graphql/ graphql-schema.json",
    "generate": "npx apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --tagName=gql",
    "serve": "serve ./dist",
    "lint": "eslint . -f codeframe",
    "typecheck": "tsc --noEmit",
    "test": "npm run typecheck && npm run lint"
  },
  "keywords": [],
  "author": "",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.3.14",
    "@stylable/runtime": "^4.0.4",
    "classnames": "^2.3.1",
    "formik": "^2.2.6",
    "graphql": "^15.5.0",
    "mobx": "^6.2.0",
    "mobx-react-lite": "^3.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "wix-style-react": "^9.101.0",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@stylable/core": "^4.0.4",
    "@stylable/webpack-plugin": "^4.0.4",
    "@types/graphql": "^14.5.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "apollo": "^2.32.5",
    "css-loader": "^5.2.1",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-stylable": "^4.0.4",
    "html-webpack-plugin": "^5.3.1",
    "node-sass": "^5.0.0",
    "resolve-url-loader": "^3.1.2",
    "rimraf": "^3.0.2",
    "sass-loader": "^11.0.1",
    "serve": "^11.3.2",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  }
}
